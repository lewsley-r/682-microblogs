      </div>\n                <h4><label v-if=\"this.blogPhoto\">File Name: {{this.blogPhoto.name}}</label></h4>\n                <button class=\"uk-button uk-button-default uk-button-primary blogBtn\" @click=\"submitForm\">\n                  Submit Blog\n                </button>\n              </fieldset>\n            </form>\n          </div>\n      </div>\n   \n  </div>\n  <div class=\"uk-child-width-expand@s uk-text-center\" uk-grid>\n     \n      <div>\n          <div class=\"uk-card uk-card-default uk-card-body\">\n            <div class=\"row align-items-center profile-header\">\n              <div class=\"col-md-2 mb-3\">\n                <img\n                  :src=\"user?.picture\"\n                  alt=\"User's profile picture\"\n                  class=\"rounded-circle img-fluid profile-picture\"\n                />\n              </div>\n              <div class=\"col-md text-center text-md-left\">\n                <h2>{{ user?.name }}</h2>\n                <p class=\"lead text-muted\">{{ user?.email }}</p>\n              </div>\n          </div>\n          </div>\n      </div>\n   \n  </div>\n  <div class=\"container\">\n    \n\n    <div class=\"row\">\n      <pre v-highlightjs><code class=\"json\">{{JSON.stringify(user, null, 2)}}</code></pre>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useAuth0 } from '@auth0/auth0-vue';\nimport { useUserStore } from '@/stores/users';\nimport { usePostsStore } from '@/stores/posts'\n\nexport default {\n  name: \"profile\",\n  setup() {\n    const auth0 = useAuth0();\n    const userStore = useUserStore();\n    const postsStore = usePostsStore();\n    \n    return {\n      user: auth0.user,\n      store: userStore,\n      postsStore: postsStore\n    }\n  },\n  data(){\n    return{\n      blogPhoto: null,\n      orgName: null,\n      postContent: null,\n    }\n  },\n  mounted(){\n    this.store.roleCheck()\n  },\n  methods: {\n    onChangePhoto(event){\n      this.blogPhoto = (event.srcElement.files[0])\n    },\n    submitForm(){\n      var microBlog = {\n        'file': this.blogPhoto,\n        'orgName': this.orgName,\n        'postContent': this.postContent,\n        'userId': this.user.sub,\n        'ownerName': this.user.nickname\n      }\n      this.postsStore.createPost(microBlog)\n    }\n  }\n};\n</script>\n\n<style scoped>\n.blogBtn{\n  margin: 1vh;\n}\n</style>\n\n"]}]}