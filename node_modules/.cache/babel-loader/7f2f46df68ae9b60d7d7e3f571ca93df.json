<div class=\"uk-card uk-card-default uk-card-body\">\n            <div class=\"row align-items-center profile-header\">\n              <div class=\"col-md-2 mb-3\">\n                <img\n                  :src=\"user?.picture\"\n                  alt=\"User's profile picture\"\n                  class=\"rounded-circle img-fluid profile-picture\"\n                />\n              </div>\n              <div class=\"col-md text-center text-md-left\">\n                <h2>{{ user?.name }}</h2>\n                <p class=\"lead text-muted\">{{ user?.email }}</p>\n              </div>\n          </div>\n          </div>\n      </div>\n      <div>\n        <div class=\"\">\n          <div class=\"js-upload\" uk-form-custom>\n                    <input type=\"file\" multiple>\n                    <button class=\"uk-button uk-button-default uk-button-primary\" accept=\"image/png, image/gif, image/jpeg\" type=\"button\" tabindex=\"-1\">\n                      Change Profile Photo\n                    </button>\n                </div>\n        </div>\n      </div>\n   \n  </div>\n  <div class=\"container\">\n    \n\n    <div class=\"row\">\n      <pre v-highlightjs><code class=\"json\">{{JSON.stringify(user, null, 2)}}</code></pre>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useAuth0 } from '@auth0/auth0-vue';\nimport { useUserStore } from '@/stores/users';\n\nexport default {\n  name: \"profile\",\n  setup() {\n    const auth0 = useAuth0();\n    const userStore = useUserStore();\n    \n    return {\n      user: auth0.user,\n      store: userStore\n    }\n  },\n  mounted(){\n    this.store.roleCheck()\n  },\n  methods: {\n    postPhoto(event){\n      console.log(event.srcElement)\n    }\n  }\n};\n</script>\n\n<style scoped>\n.blogBtn{\n  margin: 1vh;\n}\n</style>\n\n"]}]}