 useUserStore } from '@/stores/users';\n\nexport default {\n  name: \"profile\",\n  setup() {\n    const auth0 = useAuth0();\n    const userStore = useUserStore();\n    \n    return {\n      user: auth0.user,\n      store: userStore\n    }\n  },\n  mounted(){\n    this.store.roleCheck()\n  },\n  methods: {\n    postPhoto(event){\n      console.log(event.srcElement)\n    }\n  }\n};\n</script>\n\n<style scoped>\n.blogBtn{\n  margin: 1vh;\n}\n</style>\n\n"]}]}